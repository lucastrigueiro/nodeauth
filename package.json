{
  "name": "nodeauth",
  "version": "0.0.1",
  "description": "Node.js authentication service",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --transpile-only src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "build": "tsc",
    "lint": "eslint src/**/*.ts --quiet",
    "format": "prettier --write 'src/**/*.{js,ts,json,css,md}'"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/amqplib": "^0.10.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.7",
    "eslint": "^9.5.0",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.5.2",
    "typescript-eslint": "^7.13.1"
  },
  "overrides": { "eslint": "^9.5.0" }
}
